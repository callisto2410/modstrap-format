  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callisto2410/modstrap-format.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:all": "npm run build:clean && npm run build && npm run build:demo",
