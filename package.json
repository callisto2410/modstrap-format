    "sass": "^1.30.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "cleave.js": "^1.6.0"
  }
